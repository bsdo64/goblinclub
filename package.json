{
  "name": "goblin-alt",
  "version": "1.0.0",
  "description": "![](https://travis-ci.org/GoblinWorld/goblinclub.svg?branch=master)",
  "main": "webpack-dev-server.js",
  "bin": {
    "goblin-alt": "apiServer.js"
  },
  "scripts": {
    "start": "npm run redis & npm run front-build & npm run pm2",
    "pm2": "DEBUG='pm2' pm2 start GoblinclubSystem.json --no-daemon",
    "redis": "redis-server ./redis.conf",
    "mysql": "mysql.server start",
    "kibana": "./debug/kibana-4.3.1-linux-x64/bin/kibana",
    "elastic": "./debug/elasticsearch-2.1.1/bin/elasticsearch",
    "debugs": "npm run elastic && npm run kibana",
    "front-build": "webpack --colors --progress --watch",
    "test": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --recursive",
    "test1": "jest --coverage",
    "lint": "eslint ./scripts -c ./scripts/.eslintrc"
  },
  "jest": {
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
